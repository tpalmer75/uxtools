    link(href='https://fonts.googleapis.com/css?family=Slabo+13px|Roboto+Condensed:400,700,400italic' rel='stylesheet' type='text/css')
    link(href="css/style.css" rel="stylesheet" type="text/css")
  body
    header
      h1 uxtools
      .wrapper.top-wrapper
        .left
          img#logo(src="img/uxtools-logo.svg" alt="UX Tools")
        .middle
          .scroll-tabs
            ul
              li(ui-sref-active="active")
                a(ui-sref="prototyping") Prototyping
              li(ui-sref-active="active")
                a(ui-sref="collaboration") Collaboration
              li(ui-sref-active="active")
                a(ui-sref="monitoring") Monitoring
              li(ui-sref-active="active")
                a(ui-sref="usability-testing") Usability Testing
        .right
          a(href="http://www.twitter.com/@_taylorpalmer") 
            div(class="icon" ng-include="'img/icons/twitter.svg'" title="Follow me on Twitter!")
            span @_taylorpalmer
    section
      .wrapper.shadow-wrapper(ui-view="")
        //- injected Angular Views
        //- div.loading-container Loading...
                
